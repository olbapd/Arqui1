module Color_Generator(input logic clk,
							  output logic [23:0] rgb);
							  


assign rgb = 0;
							  
always @(posedge clk)
begin
	//	When H_Sync Re-start
	if(clk)
	begin
		//	V_Sync Counter
		if( V_Cont < V_SYNC_TOTAL )
			V_Cont	<=	V_Cont+1;
		else
			V_Cont	<=	0;
		//	V_Sync Generator
		if(	V_Cont < V_SYNC_CYC )
			mVGA_V_SYNC	<=	0;
		else
			mVGA_V_SYNC	<=	1;
	end
end


							  
endmodule
