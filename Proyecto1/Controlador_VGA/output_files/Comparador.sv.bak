module Comparator(input logic clk,reset,
						input logic [9:0] hcount, vcount,
						output logic [1:0] cuadrante);
						



always @(posedge clk)
begin
	if(hcount < 450)
	begin
		if(vcount < 294)
		begin
			cuadrante <= 0;
		end
	end
	if(hcount > 450)
	begin
		if(vcount < 294)
		begin
			cuadrante <= 1;
		end
	end
	if(hcount < 450)
	begin
		if(vcount > 294)
		begin
			cuadrante <= 2;
		end
	end
	if(hcount > 450)
	begin
		if(vcount > 294)
		begin
			cuadrante <= 3;
		end
	end
end


endmodule
